{
  "name": "ggpp",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "clean": "rimraf ./build",
    "start": "npm run clean && npm run lint && node ./src/ggpp.js",
    "dev": "npm run clean && npm run lint && nodemon --watch ./src ./src/ggpp.js",
    "build": "npm run clean && npm run lint && pkg ./src/ggpp.js --targets node8-linux-x64,node8-macos-x64,node8-win-x64 --out-path ./build",
    "lint": "eslint -c ./package.json ./"
  },
  "engines": {
    "node": "^8.0.0"
  },
  "author": "Glenn de Haan",
  "license": "MIT",
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "comma-dangle": [
        "error",
        "never"
      ],
      "indent": [
        "error",
        4
      ]
    },
    "extends": [
      "eslint:recommended"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "commander": "^3.0.1",
    "express": "^4.17.1",
    "node-json-db": "^1.0.1",
    "pkg": "^4.4.0",
    "rimraf": "^3.0.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "nodemon": "^1.19.2"
  }
}
